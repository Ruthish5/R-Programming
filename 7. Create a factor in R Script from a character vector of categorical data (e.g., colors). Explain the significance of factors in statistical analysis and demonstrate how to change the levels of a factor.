colors <- c("Red", "Blue", "Green", "Red", "Blue", "Green", "Red", "Green")
color_factor <- factor(colors)
cat("Factor levels:\n")
print(color_factor)
levels(color_factor) <- c("Dark Red", "Sky Blue", "Lush Green")
cat("\nModified Factor Levels:\n")
print(color_factor)
cat("\nCount of each level:\n")
print(table(color_factor))
